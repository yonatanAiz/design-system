{
  "name": "library",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/main.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "storybook": "start-storybook -p 6006",
    "start:build": "start-storybook -p 6006 -s ./storybook-static",
    "build-storybook": "build-storybook",
    "test": "jest --ignore storybook.spec.ts",
    "test:watch": "jest --watch",
    "test:staged": "jest --bail --findRelatedTests",
    "test:storyshots": "jest --config jest.storyshots.config.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "storyshoots": "yarn build-storybook && start-server-and-test start:build http://localhost:6006 test:storyshots"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storyshots": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "start-server-and-test": "^1.15.3",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">= 17.0.2",
    "react-dom": ">= 17.0.2"
  },
  "resolutions": {
    "react-test-renderer": "18.1.0"
  },
  "dependencies": {
    "@storybook/addon-storyshots-puppeteer": "^6.5.16",
    "classnames": "^2.3.2",
    "puppeteer": "^19.6.3"
  }
}
